{
  "extends": "@tsconfig/ember/tsconfig.json",
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "test-app/tests/*": ["tests/*"],
      "test-app/*": ["app/*"],
      "tracked-built-ins": ["../addon/addon"],
      "tracked-built-ins/*": ["../addon/addon/*"],
      "tracked-built-ins/test-support": ["../addon/addon-test-support"],
      "tracked-built-ins/test-support/*": ["../addon/addon-test-support/*"],
      "*": ["types/*"]
    }
  },
  "include": ["app/**/*", "tests/**/*", "types/**/*"]
}
